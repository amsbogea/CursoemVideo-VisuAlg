algoritmo "somadorNumerico"
// Função :
// Autor :
// Data : 25/4/2017
// Seção de Declarações 
var
  cont,n,s: inteiro
inicio
 cont <-1
 s <-0
 enquanto (cont <=10) faca
    escreva ("Digite o",cont,"o. valor: ")
    leia (N)
    s<-s+n
    cont <-cont+1
 fimenquanto
 escrevaL("")
 escrevaL ("a soma dos numeros eh: ",s)
fimalgoritmo
