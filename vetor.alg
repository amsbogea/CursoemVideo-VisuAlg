algoritmo "vetor"
// Função :
// Autor :
// Data : 26/04/2018
// Seção de Declarações 
var
   notas: vetor [0..9] de real
   minhanota: real
inicio
// Seção de Comandos 
notas[0] <- 8.5
minhanota <- notas[0]
escreval ("O vetor, na posição do índice 0 contém: ", notas[0])
escreval ("A variável contém: ", minhanota)
fimalgoritmo
